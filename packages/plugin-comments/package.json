{
  "name": "@noxion/plugin-comments",
  "version": "0.1.0",
  "description": "Comments plugin for Noxion with Supabase backend",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "lint": "echo 'No linting for plugin-comments package'",
    "lint:fix": "echo 'No linting for plugin-comments package'",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "noxion",
    "plugin",
    "comments",
    "supabase"
  ],
  "author": "Noxion Team",
  "license": "MIT",
  "dependencies": {
    "@noxion/core": "workspace:*",
    "@noxion/types": "workspace:*",
    "@supabase/supabase-js": "^2.39.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "next": "14.2.5",
    "tsup": "^8.0.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}